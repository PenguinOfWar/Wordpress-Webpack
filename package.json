{
  "name": "wordpress-webpack",
  "version": "1.0.0",
  "description": "A vagrant, php, node and webpack starter kit for bootstrapping Wordpress sites with a pleasant development environment",
  "engines": {
    "node": ">=12.15.0"
  },
  "engineStrict": true,
  "main": "src/client/index.js",
  "scripts": {
    "watch": "NODE_ENV=development BUILD_ENV=development webpack-dev-server -d --port 5000 --inline --colors",
    "build": "NODE_ENV=production BUILD_ENV=production webpack --progress --bail",
    "release": "NODE_ENV=production BUILD_ENV=production RELEASE=true webpack --progress --bail"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PenguinOfWar/Wordpress-Webpack.git"
  },
  "keywords": [
    "framework",
    "wordpress",
    "webpack"
  ],
  "author": "Darryl Walker <darryljwalker@gmail.com>",
  "license": "MIT",
  "private": true,
  "bugs": {
    "url": "https://github.com/PenguinOfWar/Wordpress-Webpack/issues"
  },
  "homepage": "https://github.com/PenguinOfWar/Wordpress-Webpack#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "colors": "^1.1.2",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.3",
    "file-exists": "^5.0.1",
    "fs": "0.0.1-security",
    "glob": "^7.1.2",
    "html-webpack-plugin": "^3.2.0",
    "imports-loader": "^0.8.0",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^0.9.0",
    "ncp": "^2.0.0",
    "node-sass": "^4.7.2",
    "nodemon": "^2.0.2",
    "postcss-easy-import": "^3.0.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-scss": "^2.0.0",
    "sass-lint": "^1.12.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "webpack": "^4.0.1",
    "webpack-assets-manifest": "^3.1.1",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.1.0",
    "webpack-watch-livereload-plugin": "0.0.1",
    "write-file-webpack-plugin": "^4.2.0"
  },
  "dependencies": {
    "jquery": "^3.4.1"
  }
}
